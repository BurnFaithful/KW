package youngmin.util;

import youngmin.calc.YoungminCalc;

public class PerfectCalc extends YoungminCalc {
	
	public float Division(int x, int y)
	{
		float result = 0;
		
		try
		{
			result = x / y;
		}
		catch (ArithmeticException e)
		{
			System.out.println("0으로는 나눌 수 없음.");
			result = 1;
		}
		
		return result;
	}
}
