package Week07;

public class A01_Array {
	public static void main(String[] args) {
		
		int[] arr = new int[10];
		
		for (int i = 0; i < arr.length; i++) {
			System.out.println(i);
		}
		
		int[] sco = {0, 1, 2, 3, 4};
		
		for (int i = 0; i < sco.length; i++) {
			System.out.println(i);
		}
	}
}
